{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021, // Use "latest" here as string
    "sourceType": "module" // This enables ES modules
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
